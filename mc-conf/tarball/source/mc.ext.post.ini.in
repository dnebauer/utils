[Include/dn_ebook]
Open=(ebook-viewer %f >/dev/null 2>&1 &)
View=(sensible-editor %f >/dev/null 2>&1 &)

# LibreOffice (only those extensions missed by default mechanism)

[Include/dn_libreoffice]
