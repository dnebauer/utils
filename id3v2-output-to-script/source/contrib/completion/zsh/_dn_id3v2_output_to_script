#compdef dn-id3v2-output-to-script

local rc=1

_arguments -S \
  - 'help_option' \
    '(-h --help)'{-h,--help}'[Show a help message]' \
  - 'script_options' \
    '(-i --input-file)'{-i,--input-file}'[Input file]:input:_files' \
    '(-o --output-file)'{-o,--output-file}'[Output file]:output:_files' \
  && rc=0

return rc
