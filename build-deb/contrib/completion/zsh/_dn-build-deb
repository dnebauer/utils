#compdef dn-build-deb

local rc=1

_arguments -S \
  - 'help_compact' \
    '-h[Show a compact help message]' \
  - 'script_options' \
    '-r[Build tree root dir (required if using -t)]:build tree root:_files' \
    '-n[Package maintainer name (required if using -t)]' \
    '-e[Package maintainer email address (required if using -t)]' \
    '-p[Package name (required if using -t)]' \
    '-t[Create empty project template]' \
  && rc=0

return rc
