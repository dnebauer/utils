#compdef dn-mp3-file-rename

local rc=1

_arguments -S \
  - 'help_option' \
    '(-h --help)'{-h,--help}'[Show a help message]' \
    '--usage[Show a short help message]' \
    '--man[Show the manual]' \
  - 'script_options' \
    '(-f --format)'{-f,--format}'[File name format template (required)]:mp3:' \
  && rc=0

return rc
