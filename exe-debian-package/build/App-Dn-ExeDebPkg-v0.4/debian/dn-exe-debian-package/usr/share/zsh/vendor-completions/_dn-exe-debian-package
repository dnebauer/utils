#compdef dn-exe-debian-package

local rc=1

_arguments -S \
  - 'help_option' \
    '(-h --help)'{-h,--help}'[Show a help message]' \
    '--usage[Show a short help message]' \
    '--man[Show the manual]' \
  - 'script_options' \
    '(-e --exe)'{-e,--exe}'[Executable name (string, required)]:exe:' \
  && rc=0

return rc
